[run]
source = src
omit =
    */tests/*
    */__pycache__/*
    */venv/*
    */.venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    @property
    @abstractmethod

[html]
directory = htmlcov
title = Payment System Coverage Report